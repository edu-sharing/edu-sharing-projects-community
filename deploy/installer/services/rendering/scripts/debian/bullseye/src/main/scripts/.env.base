########################################################################################################################
### repository-service
### for the automatic registration of the rendering-service with edu-sharing
########################################################################################################################

# --- edu-sharing host
REPOSITORY_SERVICE_HOST=localhost

# --- edu-sharing port
REPOSITORY_SERVICE_PORT=80

# --- edu-sharing admin password
REPOSITORY_SERVICE_ADMIN_PASS=admin



